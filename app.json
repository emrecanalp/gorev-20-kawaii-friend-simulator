{
  "expo": {
    "name": "Kawaii Friend",
    "slug": "kawaii-friend-simulator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF5F7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.kawaiifriend"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFF5F7"
      },
      "package": "com.yourname.kawaiifriend"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FFB6C1"
        }
      ]
    ]
  }
}